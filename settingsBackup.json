  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "No italics",
        "scope": [
          "comment",
          "keyword",
          "variable.language",
          "meta.function-call",
          "meta.block",
          "storage",
          "entity.name.type",
          "entity.other.attribute-name",
          "keyword.control.import",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.control.export",
          "keyword.control.default",
          "storage.type.function",
          "storage.modifier.async",
          "storage.type.type",
          "storage.type",
          "meta.var.expr storage.type",
          "storage.modifier",
          "keyword.operator.new",
          "keyword.operator.delete",
          "storage.modifier.static",
          "storage.modifier.private",
          "storage.modifier.protected",
          "keyword.control.conditional",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.jsx",
          "entity.other.attribute-name.tsx",
          "comment.line.double-slash",
          "comment.line.double-slash.documentation",
          "variable.parameter",
          "punctuation",
          "meta.brace.round",
          "punctuation.definition.block",
          "keyword.control.as",
          "variable",
          "string.quoted",
          "meta.jsx.children",
          "variable.other.object",
          "variable.other.constant",
          "support.class.component",
          "variable.other.object.property"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end",
          "punctuation",
          "meta.brace.round",
          "source",
          "text.html.derivative",
          "keyword.operator.comparison",
          "keyword.operator.ternary",
          "keyword.operator.assignment",
          "keyword.operator.logical",
          "keyword.operator.relational",
          "keyword.operator.arithmetic",
          "keyword.operator.type.annotation",
          "punctuation.separator.key-value",
          "keyword.operator.type",
          "meta.brace.square",
          "meta.arrow",
          "storage.type.function.arrow",
          "punctuation.section.embedded.begin",
          "punctuation.section.embedded.end",
          "meta.jsx.children",
          "storage.type.function.arrow"
        ],
        "settings": {
          "foreground": "#e6e6e6",
          "fontStyle": ""
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#a2a2a2",
          "fontStyle": ""
        }
      }
    ],
    "[Min Theme+]": {
      "textMateRules": [
        {
          "name": "TYPES",
          "scope": [
            "support.type.primitive.tsx",
            "support.type.primitive.jsx",
            "support.type.primitive.js",
            "support.type.primitive.ts",
            "entity.name.type"
          ],
          "settings": {
            "foreground": "#D19A66"
          }
        },
        {
          "scope": [
            "variable.other.object",
            "variable.other.object",
            "variable.other.constant.object",
            "variable.other.property"
          ],
          "settings": {
            "foreground": "#E06C75"
          }
        },
        {
          "scope": ["string.quoted", "string.template", "string", "source.ini"],
          "settings": {
            "foreground": "#41B883"
          }
        },
        {
          "scope": [
            "variable.other.object.property",
            "support.variable.property",
            "variable.other.property",
            "variable.other.constant.property"
          ],
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "name": "BLUE",
          "scope": ["source meta.export.default variable.other.readwrite"],
          "settings": {
            "foreground": "#61AFEF"
          }
        },
        {
          "name": "PURPLE",
          "scope": ["variable.language.this"],
          "settings": {
            "foreground": "#C678DD"
          }
        },
        {
          "scope": ["meta.jsx.children", "support.type.property-name.css"],
          "settings": {
            "foreground": "#fefefe"
          }
        },
        {
          "name": "WHITE",
          "scope": [
            "punctuation.definition.typeparameters.begin.ts",
            "punctuation.definition.typeparameters.begin.tsx",
            "punctuation.definition.typeparameters.begin.js",
            "punctuation.definition.typeparameters.begin.jsx",
            "punctuation.definition.typeparameters.end.ts",
            "punctuation.definition.typeparameters.end.tsx",
            "punctuation.definition.typeparameters.end.js",
            "punctuation.definition.typeparameters.end.jsx",
            "punctuation.separator.parameter.ts",
            "punctuation.separator.parameter.tsx",
            "punctuation.separator.parameter.jsx",
            "punctuation.separator.parameter.js",
            "punctuation.definition.template-expression",
            "punctuation.definition.string.template",
            "punctuation.terminator.statement.ts",
            "punctuation.terminator.statement.tsx",
            "punctuation.terminator.statement.js",
            "punctuation.terminator.statement.jsx",
            "punctuation.definition.template-expression.begin",
            "punctuation.definition.template-expression.end",
            "keyword.operator.terraform",
            "keyword.operator.list_type.prisma"
          ],
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
