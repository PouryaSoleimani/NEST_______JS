  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "No italics",
        "scope": [
          "comment",
          "keyword",
          "variable.language",
          "meta.function-call",
          "meta.block",
          "storage",
          "entity.name.type",
          "entity.other.attribute-name",
          "keyword.control.import",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.control.export",
          "keyword.control.default",
          "storage.type.function",
          "storage.modifier.async",
          "storage.type.type",
          "storage.type",
          "meta.var.expr storage.type",
          "storage.modifier",
          "keyword.operator.new",
          "keyword.operator.delete",
          "storage.modifier.static",
          "storage.modifier.private",
          "storage.modifier.protected",
          "keyword.control.conditional",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.jsx",
          "entity.other.attribute-name.tsx",
          "comment.line.double-slash",
          "comment.line.double-slash.documentation",
          "variable.parameter",
          "punctuation",
          "meta.brace.round",
          "punctuation.definition.block",
          "keyword.control.as",
          "punctuation.definition.comment",
          "keyword.control.trycatch",
          "storage.type.class",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end",
          "meta.tag",
          "punctuation",
          "meta.brace.round",
          "source",
          "text.html.derivative",
          "keyword.operator.comparison",
          "keyword.operator.ternary",
          "keyword.operator.assignment",
          "keyword.operator.logical",
          "keyword.operator.relational",
          "keyword.operator.arithmetic",
          "keyword.operator.type.annotation",
          "punctuation.separator.key-value",
          "keyword.operator.type",
          "meta.brace.square",
          "meta.arrow",
          "storage.type.function.arrow",
          "punctuation.section.embedded.begin",
          "punctuation.section.embedded.end",
          "meta.jsx.children",
          "storage.type.function.arrow"
        ],
        "settings": {
          "foreground": "#d9d9d9",
          "fontStyle": ""
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#bfbfbf"
        }
      }
    ],
    "[Meteora]": {
      "textMateRules": [
        {
          "scope": ["string.quoted"],
          "settings": {
            "foreground": "#85D0B7"
          }
        }
      ]
    },
    "[GitHub Dark Default]": {
      "textMateRules": [
        {
          "scope": [
            "entity.other.attribute-name",
            "support.type.primitive",
            "entity.name.type",
            "entity.name.type.class",
            "entity.name.function"
          ],
          "settings": {
            "foreground": "#B392F0"
          }
        },
        {
          "scope": [],
          "settings": {
            "foreground": "#d2a8ff"
          }
        },
        {
          "scope": [
            "variable.other.readwrite",
            "variable.parameter",
            "variable.object.property",
            "variable.other.object"
          ],
          "settings": {
            "foreground": "#79C0FF"
          }
        },
        {
          "scope": ["variable.other.object.property", "variable.other.readwrite.alias"],
          "settings": {
            "foreground": "#FAFAFA",
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "punctuation.definition.template-expression",
            "punctuation.separator.key-value",
            "punctuation.separator.comma",
            "punctuation.accessor",
            "keyword.operator.assignment.compound",
            "keyword.operator.assignment",
            "meta.type.annotation",
            "keyword.operator.comparison",
            "keyword.operator.ternary",
            "keyword.operator.arithmetic",
            "keyword.operator.logical",
            "storage.type.function.arrow",
            "keyword.operator.type",
            "keyword.operator.type.annotation"
          ],
          "settings": {
            "foreground": "#F97583",
            "fontStyle": ""
          }
        }
      ]
    },
    "[GitHub Dark High Contrast]": {
      "textMateRules": [
        {
          "scope": [
            "entity.name.type.class",
            "entity.other.attribute-name",
            "support.type.primitive",
            "entity.name.type",
            "constant.language.boolean",
            "constant.language.null",
            "constant.language.void",
            "constant.language.undefined"
          ],
          "settings": {
            "foreground": "#d2a8ff"
          }
        },
        {
          "scope": [],
          "settings": {
            "foreground": "#A5D6FF"
          }
        },
        {
          "scope": [
            "variable.other.readwrite",
            "variable.parameter",
            "variable.object.property",
            "variable.other.object"
          ],
          "settings": {
            "foreground": "#79C0FF"
          }
        },
        {
          "scope": ["variable.other.object.property", "variable.other.readwrite.alias"],
          "settings": {
            "foreground": "#E6EDF3"
          }
        },
        {
          "scope": [
            "punctuation.definition.template-expression",
            "punctuation.separator.key-value",
            "punctuation.separator.comma",
            "punctuation.accessor",
            "keyword.operator.assignment.compound",
            "keyword.operator.assignment",
            "meta.type.annotation",
            "keyword.operator.comparison",
            "keyword.operator.ternary",
            "keyword.operator.arithmetic",
            "keyword.operator.logical",
            "storage.type.function.arrow",
            "keyword.operator.type",
            "keyword.operator.type.annotation"
          ],
          "settings": {
            "foreground": "#F97583",
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[Next Dark Pro]": {
      "textMateRules": [
        {
          "name": "TYPES",
          "scope": [
            "support.type.primitive.tsx",
            "support.type.primitive.jsx",
            "support.type.primitive.js",
            "support.type.primitive.ts",
            "entity.name.type"
          ],
          "settings": {
            "foreground": "#D19A66"
          }
        },
        {
          "name": "RED",
          "scope": ["source meta.export.default variable.other.readwrite"],
          "settings": {
            "foreground": "#61AFEF"
          }
        },
        {
          "name": "RED",
          "scope": [
            "source meta.decorator meta.objectliteral meta.object.member meta.object-literal.key",
            "variable.other.constant.property"
          ],
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "scope": ["meta.jsx.children"],
          "settings": {
            "foreground": "#fefefe"
          }
        },
        {
          "scope": ["variable.other.object"],
          "settings": {
            "foreground": "#EF596F"
          }
        },
        {
          "scope": ["variable.language.this"],
          "settings": {
            "foreground": "#C678DD"
          }
        }
      ]
    }
  },
